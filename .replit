modules = ["python-3.11", "nodejs-20", "postgresql-16", "python3"]
[agent]
expertMode = true
integrations = ["python_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "file", "freetype", "glibcLocales", "lcms2", "libGL", "libGLU", "libiconv", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "poppler-utils", "poppler_utils", "rustc", "tcl", "tesseract", "tk", "zlib"]

[workflows]
